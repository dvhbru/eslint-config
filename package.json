{
  "name": "@dvhb/eslint-config",
  "version": "1.0.4",
  "description": "dvhb eslint config",
  "main": "index.js",
  "files": [
    "src",
    "index.js"
  ],
  "keywords": [
    "dvhb",
    "eslint"
  ],
  "scripts": {
    "test": "eslint .",
    "fix": "yarn test --fix",
    "print": "eslint --print-config .",
    "precommit": "lint-staged"
  },
  "author": "Aleksey Subbotenko <asu@dvhb.ru>",
  "license": "MIT",
  "dependencies": {
    "babel-eslint": "^10.0.2",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier-eslint": "^9.0.0"
  },
  "devDependencies": {
    "eslint": "^6.0.1",
    "husky": "^3.0.0",
    "lint-staged": "^8.2.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
